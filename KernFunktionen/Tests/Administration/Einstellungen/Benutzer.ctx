    with [get-group Verrechnung] {
        get-group Rollenzuordnung | get-table | get-item -path fhir | check
        get-group "t채tig f체r" | get-table | get-item -path $VerantwortlicherArzt | check
    }
    get-window "No contact assigned" | get-button OK | click
    get-group Verrechnung | get-link "Nicht gesetzt <a>채ndern</a>" | click-link
    with [get-window "Kontakt ausw채hlen"] {
        get-table | select  $VerantwortlicherArzt
